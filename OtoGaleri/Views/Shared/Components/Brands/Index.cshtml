@model System.Collections.Generic.IEnumerable<string>

<div class="mt-4">
    <h1 class="mb-3 bg bg-primary text-center fw-bold text-white"> Markalarımız </h1>
    <div class="row">
        @foreach (var carModel in Model)
        {
            <div class="mb-3 ml-1 mr-3 border border-primary px-1 py-3 col-md-3">
               <span><img width="25" height="25" src="~/Assets/assets/img/@(carModel).png" alt=""></span> <a asp-route-id="@carModel" asp-controller="Car" asp-action="Index">
                    @carModel
                </a>
            </div>
        }
    </div>
</div>